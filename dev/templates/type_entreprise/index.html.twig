{% extends 'base.html.twig' %}

{% block title %}TypeEntreprise index{% endblock %}

{% block body %}
    <h1>TypeEntreprise index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_entreprise in type_entreprises %}
            <tr>
                <td>{{ type_entreprise.id }}</td>
                <td>{{ type_entreprise.nom }}</td>
                <td>
                    <a href="{{ path('type_entreprise_show', {'id': type_entreprise.id}) }}">show</a>
                    <a href="{{ path('type_entreprise_edit', {'id': type_entreprise.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('type_entreprise_new') }}">Create new</a>
{% endblock %}
